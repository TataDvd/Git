<Window x:Class="BarCodeGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BarCodeGenerator"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="5,5,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" KeyDown="textBox_KeyDown"/>
        <TextBox x:Name="BarBox" HorizontalAlignment="Left"  Margin="5,55,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"  FontFamily="/BarCodeGenerator;component/#Free 3 of 9 Extended" FontSize="36"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="0,127,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" KeyDown="textBox1_KeyDown"/>
        <TextBox x:Name="BarBox1" HorizontalAlignment="Left"  Margin="5,155,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"  FontFamily="/BarCodeGenerator;component/#Free 3 of 9 Extended" FontSize="36"/>
        <Button Width="100" Height="30" Click="Button_Click">Print</Button>
    </Grid>
</Window>
